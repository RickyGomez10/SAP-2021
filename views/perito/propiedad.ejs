<h1>Propiedad de ejemplo</h1>
<div class="row">
    <div class="col-12 col-lg-6">
        <p class="mt-5">Nombre: Nombre de propiedad de ejemplo</p>
        <p class="mt-4">Dirección: Dirección de propiedad de ejemplo</p>
        <p class="mt-4">Departamento: Departamento de propiedad de ejemplo</p>
        <p class="mt-4">Municipio: Municipio de propiedad de ejemplo</p>
        <p class="mt-4">Metros cuadrados: 100</p>
        <p class="mt-4">Propietario: Propietario de propiedad de ejemplo</p>
    </div>
    <div class="col-12 col-lg-6">
        <img class="img-thumbnail" src="../../public/assets/img/bg-masthead.jpg" />
    </div>
</div>
<h4 class="mt-3">Avalúos de la propiedad</h4>
<table class="table table-hover mt-5">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Nombre</th>
        <th scope="col">Dirección</th>
        <th scope="col">Fecha</th>
        <th scope="col">Estado</th>
        <th scope="col">Opciones</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < lista.length ; i++) { %>
        <tr>
          <th><%= i+1 %></th>
          <td> <%= lista[i].user %> </td>
          <td> <%= lista[i].password %> </td>
          <td> <%= lista[i].rol %> </td>
          <td> <%= lista[i].user %> </td>
          <td> <a href=<%= "/perito/avaluo/"+lista[i].user %>>Ver</a> </td>
        </tr>
      <% } %>
      <tr>
        <th>☼</th>
        <td>Avalúo 254</td>
        <td>Calle Don Bosco y Av. Manuel Gallardo, 1-1, Santa Tecla</td>
        <td>15/8/2021</td>
        <td>Completado</td>
        <td>Ver</td>
      </tr>
    </tbody>
  </table>
